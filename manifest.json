{
  "manifest_version": 3,
  "name": "ByteSense",
  "description": "In Development",
  "version": "1.0",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "src/popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/watch/*"],
      "js": ["src/content_script/script.js"]
    }
  ]
}
