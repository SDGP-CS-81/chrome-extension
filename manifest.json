{
  "manifest_version": 3,
  "name": "tester",
  "description": "Base Level Extension",
  "version": "1.0",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "src/index.html"
  },
  "content_scripts": [
    {
      "matches": ["www.youtube.com/watch/*"],
      "js": ["src/script.js"]
    }
  ]
}
